#include<stdio.h>
#include<math.h>

int power(int x, int n) {
    int res = pow(x, n);
    return res;
}

int main() {
    int a, b;
    printf("Enter the first value: ");
    scanf("%d",&a);
    printf("Enter the second value: ");
    scanf("%d",&b);
    printf("%d power of %d is: %d", a, b, power(a, b));
    return 0;
}
