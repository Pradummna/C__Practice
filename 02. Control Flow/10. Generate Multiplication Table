#include<stdio.h>
int main() {
    int input, range;
    printf("Enter the number to get the multiplication table of that number\n");
    scanf("%d", &input);
    printf("Enter the range for your multiplication table\n");
    scanf("%d", &range);
    for(int i=1; i<=range; i++) {
        printf("%d * %d = %d\n", input, i, input*i);
    }
    return 0;
}
