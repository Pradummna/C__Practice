// Note: This problem can sloved easily but I write this as for fun
#include<stdio.h>
int main() {
  int a, b, c;
  int max;

  printf("Enter three random value\n");
  for(int i=1; i<=3; i++) {
    printf("Value %d: ",i);
    if(i == 1) {
      scanf("%d", &a);
    } else if(i == 2) {
      scanf("%d", &b);
    } else {
      scanf("%d", &c);
    }
  }

  printf("\nValues are: %d %d %d",a, b, c);
  
  max = a;
  if(max < b) {
    max = b;
  }
  else if(max < c) {
    max = c;
  }

  printf("\nMaximum values is: %d", max);
  return 0;
}
